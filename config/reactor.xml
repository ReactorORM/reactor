<reactor>
	<!-- <config>
		<dsn value="vacationAuctions" />
		<type value="mssql" />
		<mapping value="/reactorData" />
		<mode value="always" />
	</config> -->

	<objects>
		<object name="Auction">
			<hasMany name="Category">
				<link name="AuctionCategory" />
			</hasMany>
			<hasMany name="Image">
				<link name="AuctionImage" />
			</hasMany>		
			<hasOne name="Region">
				<relate from="regionId" to="regionId" />
			</hasOne>
		</object>
	
		<object name="AuctionCategory">
			<hasOne name="Auction">
				<relate from="auctionId" to="auctionId" />
			</hasOne>
			<hasOne name="Category">
				<relate from="categoryId" to="categoryId" />
			</hasOne>
		</object>
		
		<object name="Category">
			<hasOne name="Image">
				<relate from="imageId" to="imageId" />
			</hasOne>
		</object>
	
		<object name="AuctionImage">
			<hasOne name="Auction">
				<relate from="auctionId" to="auctionId" />
			</hasOne>
			<hasOne name="Image">
				<relate from="imageId" to="imageId" />
			</hasOne>
		</object>
	</objects>
</reactor>