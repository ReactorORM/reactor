<reactor>
	<config>
		<project value="Scratch" />
		<dsn value="scratch" />
		<type value="mssql" />
		<mapping value="/reactorData" />
		<mode value="production" />
		<!-- These config values are not required -->
		<!-- <username value="" />
		<password value="" /> -->
	</config>

	<objects>
		<object name="user">
			<hasMany name="user" alias="relation">
				<link name="relatedUser" alias="user2" />
			</hasMany>
		</object>
		
		<object name="relatedUser">
			<hasOne name="user" alias="user1">
				<relate from="userId1" to="userId" />
			</hasOne>
			<hasOne name="user" alias="user2">
				<relate from="userId2" to="userId" />
			</hasOne>
			<hasOne name="foo">
				<relate from="fooId" to="fooId" />
			</hasOne>
		</object>
		
	</objects>
</reactor>