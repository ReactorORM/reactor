<reactor>
	<config>
		<project value="Scratch" />
		<dsn value="scratch" />
		<type value="mssql" />
		<mapping value="/reactorData" />
		<mode value="production" />
		<!-- These config values are not required -->
		<!-- <username value="" />
		<password value="" /> -->
	</config>

	<objects>
		<object name="user">
			<hasMany name="user" alias="childUser">
				<link name="relatedUser" from="parent" to="child" />
			</hasMany>
		</object>
		
		<object name="relatedUser">
			<hasOne name="user" alias="parent">
				<relate from="userId1" to="userId" />
			</hasOne>
			<hasOne name="user" alias="child">
				<relate from="userId2" to="userId" />
			</hasOne>
		</object>
		
		
		
	</objects>
</reactor>